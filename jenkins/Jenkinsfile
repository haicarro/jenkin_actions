pipeline {
    agent any

    stages {
        stage('Build') {
            environment {
                S3_BUCKET = 's3'
            }
            steps {
                withAWS(credentials: 'jenkins-aws-credentials-productions', region: 'ap-southest-1') {
                    sh 'aws s3 ls'
                }
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}